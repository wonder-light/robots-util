{
  "name": "robots-util",
  "version": "0.1.1",
  "description": "Parses the robots.txt file and other util",
  "author": "wonder-light",
  "license": "MIT",
  "homepage": "https://github.com/wonder-light/robots-util",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js",
    "default": "./dist/cjs/index.js"
  },
  "scripts": {
    "build": "rollup --config rollup.config.ts --configPlugin typescript"
  },
  "engines": {
    "node": " >= 6.1.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wonder-light/robots-util"
  },
  "bugs": {
    "url": "https://github.com/wonder-light/robots-util/issues"
  },
  "keywords": [
    "parse",
    "robots",
    "txt"
  ],
  "files": [
    "README.md",
    "dist/*"
  ],
  "devDependencies": {
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/node": "^22.9.0",
    "mocha": "~3.2.0",
    "rollup": "^4.27.2",
    "rollup-plugin-dts": "^6.1.1",
    "standard": "^8.6.0",
    "tslib": "^2.8.1"
  }
}
